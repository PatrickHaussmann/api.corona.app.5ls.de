{
  "version": 2,
  "rewrites": [
    { "source": "/districts", "destination": "/api/districts" },
    { "source": "/states", "destination": "/api/states" },
    { "source": "/", "destination": "/404" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "s-maxage=3600"
        },
        {
          "key" : "X-test",
          "value" : "hi"
        }
      ]
    }
  ]
}
